---
import BaseLayout from '@/layouts/BaseLayout.astro'
import { buttonVariants } from '@/components/ui/button'
import { Image } from 'astro:assets'
import logo from '../assets/logo.svg'
import misty from '../assets/misty.jpg'
import '../assets/fontawesome/css/all.min.css'
---

<BaseLayout
  title="PRI5M - modern development for forward thinking individuals"
  description="Modern development for forward thinking individuals"
>
  <div class="fade-bg -mt-20">
    <section
      class="gap-6pb-40 container grid items-center pt-40 max-[600px]:pt-20"
    >
      <div class="pb-40 pt-20">
        <div class="flex flex-col items-center gap-2 text-center">
          <h1
            class="max-w-[900px] animate-fade text-4xl font-extrabold animate-delay-200 md:text-6xl"
          >
            We deliver <span class="fade">unforgettable</span> marketing experiences
          </h1>
          <p
            class="text-md mb-10 mt-5 max-w-[700px] animate-fade text-center text-muted-foreground animate-delay-300 md:text-lg"
          >
            Transform your online presence with our web development expertise at
            Prism. We specialize in creating memorable, effective, and
            results-driven websites that elevate your brand to new heights in
            the digital era.
          </p>
        </div>
        <div
          class="flex animate-fade-down justify-center gap-4 animate-delay-200 animate-duration-[800ms] max-[600px]:flex-col"
        >
          <a
            href="/process"
            rel="noreferrer"
            class={buttonVariants({ variant: 'fadeintro' })}
          >
            Learn the Process
          </a>
          <a
            href="/survey"
            rel="noreferrer"
            class={buttonVariants({ variant: 'outlineintro' })}
            ><Image
              src={logo}
              alt="PRI5M Logo"
              id="site-logo"
              class="mr-3 max-w-[24px]"
            />
            Take the Survey
          </a>
        </div>
      </div>
    </section>
  </div>

